[script1]
type = pull
depends = python-beautifulsoup>=3.2.1-1
freq = 0 0 * * *
script = scripts/releases_count.py
fields = ts:DateTime, name:String, source_files:String, source_packages:String, disk_usage:String, ctags:String, sloc:String
format = csv
override_ts = true
delete_before_insert = false
graph_type = custom
source = http://sources.debian.net/stats/
description = Provides statistics about releases such as source files, source packages, disk usage, lines of code.
